{
  "name": "oss-tokyoclient",
  "version": "1.0.1",
  "description": "A Typescript client library for connecting to a Tokyo game server and controlling a ship in the game.",
  "repository": {
    "type": "git",
    "url": "https://github.com/haongo138/oss-tokyoclient.git"
  },
  "bugs": {
    "url": "https://github.com/haongo138/oss-tokyoclient/issues"
  },
  "author": "Hao Ngo <haoxray@gmail.com>",
  "license": "MIT",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "/build"
  ],
  "scripts": {
    "start:prod": "npm run build && node build/index.js",
    "start:dev": "npx nodemon",
    "build": "rimraf ./build && tsc",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "start:example": "npm run build && node examples/simple-bot.js"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@types/ws": "^8.5.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "isomorphic-ws": "^5.0.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  }
}
